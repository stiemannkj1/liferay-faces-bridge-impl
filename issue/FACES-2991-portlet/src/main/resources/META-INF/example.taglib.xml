<?xml version='1.0' encoding='UTF-8'?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
	version="2.0">
	<namespace>http://liferay.com/faces/example</namespace>
	<tag>
		<tag-name>component</tag-name>
		<component>
			<component-type>com.liferay.faces.issues.example.component.ExampleComponent</component-type>
			<renderer-type>com.liferay.faces.issues.example.component.ExampleRenderer</renderer-type>
		</component>
		<attribute>
			<name>binding</name>
			<required>false</required>
			<type>javax.faces.component.UIComponent</type>
		</attribute>
		<attribute>
			<name>id</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>rendered</name>
			<required>false</required>
			<type>boolean</type>
		</attribute>
	</tag>
</facelet-taglib>
